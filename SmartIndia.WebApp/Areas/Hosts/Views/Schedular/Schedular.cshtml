    <!--for once and recurring JS-->
    <script>

        $('#ScDate').datepicker(
            {
                format: 'dd-mmm-yy',
                disableDates: function (date) {
                    // allow for today
                    const currentDate = new Date().setHours(0, 0, 0, 0);
                    return date.setHours(0, 0, 0, 0) >= currentDate ? true : false;
                }
            });

        $("#ScDate").change(function () {
            var startdate = $("#ScDate").val();
            $("#hfonceDate").val(dateFormat(startdate, 'yyyy-mm-dd'));
        });
    </script>
    <script>

        $('#uScDate').datepicker(
            {
                format: 'dd-mmm-yy',
                disableDates: function (date) {
                    // allow for today
                    const currentDate = new Date().setHours(0, 0, 0, 0);
                    return date.setHours(0, 0, 0, 0) >= currentDate ? true : false;
                }
            });

        $("#uScDate").change(function () {
            var startdate = $("#uScDate").val();
            $("#uSccDate").val(dateFormat(startdate, 'yyyy-mm-dd'));
        });
    </script>
    <script>
        $('#datepickerFrom').datepicker({
            format: 'dd-mmm-yy',
            disableDates: function (date) {
                // allow for today
                const currentDate = new Date().setHours(0, 0, 0, 0);
                return date.setHours(0, 0, 0, 0) >= currentDate ? true : false;
            }
        });
        $("#datepickerFrom").change(function () {
            var fromdate = $("#datepickerFrom").val();
            $("#SccDate").val(dateFormat(fromdate, 'yyyy-mm-dd'));
        });
    </script>
    <script>
        $('#datepickerTo').datepicker({
            format: 'dd-mmm-yy',
            disableDates: function (date) {
                // allow for today
                const currentDate = new Date().setHours(0, 0, 0, 0);
                return date.setHours(0, 0, 0, 0) >= currentDate ? true : false;
            }
        });
        $("#datepickerTo").change(function () {
            var enddate = $("#datepickerTo").val();
            $("#SccTo").val(dateFormat(enddate, 'yyyy-mm-dd'));
        });
    </script>
    <script>
        $('input[type=radio][name=rbtOnce]').change(function () {
            if (this.value == 'once') {
                $("#divdailyrecurring").css("display", "none");
                $("#dtponce").css("display", "block");

            }
            else if (this.value == 'recurring') {
                $("#dtponce").css("display", "none");
                $("#divDayofweek").css("display", "none");
                $("#divdailyrecurring").css("display", "block");
            }
        });
    </script>
    <script>
        $(document).ready(function () {
            $("#ddlrecurringschedule").change(function () {

                var selectedValue = $(this).children("option:selected").val();

                if (selectedValue == '0') {
                    $("#divDayofweek").css("display", "none");
                    $("#divDayofMonth").css("display", "none");
                    $("#divmonthdate").css("display", "none");
                    $("#divdateforweekly").css("display", "none");


                }
                if (selectedValue == 'Daily') {
                    $("#divDayofweek").css("display", "none");
                    $("#divDayofMonth").css("display", "none");
                    $("#divmonthdate").css("display", "none");
                    $("#divdateforweekly").css("display", "block");
                }
                if (selectedValue == 'Weekly') {
                    $("#divDayofweek").css("display", "block");
                    $("#divDayofMonth").css("display", "none");
                    $("#divmonthdate").css("display", "none");
                    $("#divdateforweekly").css("display", "block");
                }
                if (selectedValue == 'Monthly') {
                    $("#divDayofMonth").css("display", "block");
                    $("#divDayofweek").css("display", "none");
                    $("#divmonthdate").css("display", "block");
                    $("#divdateforweekly").css("display", "none");
                }
            });
        });
    </script>

    <script>
        $("#Duration").change(function () {
            var duration = $('option:selected', $(this)).val();
            var timestr = $("#StartTime").val();
            var now = new Date('1900-01-01 ' + timestr + ':00');
            var endtime = new Date(now.getTime() + parseInt(duration) * 60000);
            $("#hfEndTime").val(endtime.toTimeString().split(' ')[0].toString('hh:mm'));
        });
    </script>
    <script>
        $("#inputCourses").change(function () {
            //alert($('option:selected', $(this)).text());
            $("#BatchNameOnce").val('');
            $("#BatchNameOnce").focus();
        });
    </script>
<script>
    $(function () {
        $('#BatchNameOnce').on('keypress', function (e) {
            if (e.which == 32) {
                return false;
            }
        });
    });
</script>
